{
  "name": "GeneralRateModel",
  "cadet_class": "CADETProcess.processModel.GeneralRateModel",
  "description": "General rate model with pore and film diffusion",
  "scalar_parameters": [
    {
      "name": "length",
      "type": "float",
      "default": null,
      "unit": "m",
      "bounds": [0, null],
      "description": "Column length"
    },
    {
      "name": "diameter",
      "type": "float",
      "default": null,
      "unit": "m",
      "bounds": [0, null],
      "description": "Column diameter"
    },
    {
      "name": "bed_porosity",
      "type": "float",
      "default": null,
      "unit": "-",
      "bounds": [0, 1],
      "description": "Interstitial (bed) porosity"
    },
    {
      "name": "particle_porosity",
      "type": "float",
      "default": null,
      "unit": "-",
      "bounds": [0, 1],
      "description": "Intraparticle porosity"
    },
    {
      "name": "particle_radius",
      "type": "float",
      "default": null,
      "unit": "m",
      "bounds": [0, null],
      "description": "Particle radius"
    },
    {
      "name": "axial_dispersion",
      "type": "float",
      "default": null,
      "unit": "m²/s",
      "bounds": [0, null],
      "description": "Axial dispersion coefficient"
    },
    {
      "name": "flow_direction",
      "type": "int",
      "default": 1,
      "unit": "-",
      "bounds": [-1, 1],
      "description": "Flow direction (1=forward, -1=backward)"
    }
  ],
  "component_parameters": [
    {
      "name": "film_diffusion",
      "type": "float",
      "default": null,
      "unit": "m/s",
      "bounds": [0, null],
      "description": "Film mass transfer coefficient"
    },
    {
      "name": "pore_diffusion",
      "type": "float",
      "default": null,
      "unit": "m²/s",
      "bounds": [0, null],
      "description": "Pore diffusion coefficient"
    },
    {
      "name": "surface_diffusion",
      "type": "float",
      "default": null,
      "unit": "m²/s",
      "bounds": [0, null],
      "description": "Surface diffusion coefficient"
    },
    {
      "name": "pore_accessibility",
      "type": "float",
      "default": null,
      "unit": "-",
      "bounds": [0, 1],
      "description": "Fraction of pores accessible to component"
    }
  ]
}
